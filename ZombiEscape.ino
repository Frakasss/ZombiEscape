void displayAmmo(int ammo){
  if(ammo == 0)  {
    gb.display.cursorX = 57;
    gb.display.cursorY = 42;
    gb.display.print("Reload!");}
  else{
    for(int i=0;i<ammo;i++){
      gb.display.drawPixel((i*2)+70, 42);
      gb.display.drawPixel((i*2)+70, 43);
    }
  } 
}

